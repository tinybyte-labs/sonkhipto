    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^8.0.0",
    "@trpc/server": "^11.0.0-next-beta.251",
    "dotenv": "^16.4.1",
    "fastify": "^4.29.0",
    "fastify-cron": "^1.3.1",
    "google-auth-library": "^9.6.3",
    "htmlparser2": "^9.1.0",
    "node-html-parser": "^6.1.12",
    "puppeteer": "^24.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.13",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
